<h1 id="movie-list-title">Ironhack Cinema</h1>
<p>Click on the movie to check the showtimes!</p>
<div class="movie-display">
    {{#each movies}}
    <div class="item">
        <img src="{{this.image}}" alt="film-poster" class="movie-poster">
        <h5>{{this.title}}</h5>
        <a class="see-more" href="/movies/:id">See More</a>
    </div>
    {{/each}}
</div>